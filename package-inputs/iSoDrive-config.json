{
  "options": {
    "paths": {
      "root": "",
      "fonts": "fonts",
      "styles": "styles",
      "mbtiles": "data"
    },
    "formatQuality": {
      "jpeg": 80,
      "webp": 90,
      "pngQuantization": false,
      "png": 90
    },
    "frontPage": true,
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": true,
    "serveStaticMaps": true,
    "watermark": "ATLAS iSoDrive"
  },
  "styles": {
    "dark-matter": {
      "style": "dark-matter-local.json",
      "serve_data": true,
      "serve_rendered": true
    },
    "osm-bright": {
      "style": "osm-bright-local.json",
      "serve_data": true,
      "serve_rendered": true
    },
    "positron": {
      "style": "positron-local.json",
      "serve_data": true,
      "serve_rendered": true
    },
    "apex-style1": {
      "style": "apex-style1.json",
      "serve_data": true,
      "serve_rendered": true
    }
  },
  "data": {
    "background-vector1": {
      "mbtiles": "raleigh.mbtiles"
    },
    "background-raster1": {
      "mbtiles": "countries.mbtiles"
    }
  }
}