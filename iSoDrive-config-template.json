{
  "options": {
    "paths": {
      "root": "",
      "fonts": "fonts",
      "styles": "styles",
      "mbtiles": "data/background"
    },
    "formatQuality": {
      "jpeg": 80,
      "webp": 90,
      "pngQuantization": false,
      "png": 90
    },
    "frontPage": true,
    "maxScaleFactor": 3,
    "maxSize": 2048,
    "pbfAlias": "pbf",
    "serveAllFonts": true,
    "serveStaticMaps": true,
    "watermark": "ATLAS iSoDrive"
  },
  "styles": {
    "osm-bright": {
      "style": "osm-bright-local.json",
      "serve_data": true,
      "serve_rendered": true
    }
  },
  "data": {
    "background-vector1": {
      "mbtiles": "planet_z0_z8.mbtiles"
    }
  },
  "iso": {
    "data": {
      "baseline": "data/operational",
      "export": "data/export"
    },
    "indexers": {
      "DTED" : {
        "gdalDrivers": [ "DTED" ],
        "extensions" : [ "dt1",  "dt2" ]
      }
    }
  }
}